<div class="container body-content">

    <h1>@ViewBag.title</h1>

  

    <hr />

    <table class="job-listing">
        @if (ViewBag.jobs != null)
        {
        @foreach (var job in ViewBag.jobs)
        {
            <tr>
                <th>ID:  @job.Id <br /> Name @job.Name <br />Employer:  @job.Employer <br />Location:  @job.Location<br />Position Type:  @job.PositionType<br /> Skill:  @job.CoreCompetency </th>
            </tr>
        }}


    </table>

</div>